directory = /mnt/archive/Multimedia/Downloads
session = /mnt/archive/Multimedia/torrents/.session

schedule = watch_directory,1,1,load_start=/mnt/archive/Multimedia/torrents/*.torrent
schedule = untied_directory,5,5,stop_untied=
schedule = low_diskspace,5,60,close_low_diskspace=1M
schedule = ratio,10,10,"stop_on_ratio=50,50M,50"

port_range = 10000-10010
upload_rate = 50

check_hash = true
encryption = require,require_RC4
dht = auto

