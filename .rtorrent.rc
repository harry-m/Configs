directory = /mnt/archive/Multimedia/To\ Sort 
session = /mnt/archive/Multimedia/torrents/.session

schedule = watch_directory,5,5,load_start=/mnt/archive/Multimedia/torrents/*.torrent
schedule = untied_directory,5,5,stop_untied=
schedule = low_diskspace,5,60,close_low_diskspace=1M
schedule = ratio,60,60,"stop_on_ratio=100,100M,100"
upload_rate = 40

check_hash = true
encryption = require_RC4
dht = auto

